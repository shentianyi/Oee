<script type="text/javascript">
    $(document).ready(function () {
        data_upload("#item-data-uploader");
    });
</script>
<p id="notice">
  <%= notice %>
</p>

<h1>上传<strong>标准工时信息</strong>文件</h1>
<hr/>
<form>
  <fieldset>
    <legend>标准工时信息表</legend>
    <div>
      <b>选择数据文件：</b>
      <input id="item-data-uploader" type="file" name="files[]" data-url="/work_times/import"/>


      <hr/>
      <div id="item-data-uploader-preview"></div>
      <div>
        <b>下载模板：</b>
        <a href="/files/<%= ImportTemplate.work_time_excel_template %>">
          <%= ImportTemplate::WORK_TIME_EXCEL_TEMPLATE %></a>
      </div>
    </div>
  </fieldset>
</form>