<p id="notice"><%= notice %></p>

<div id="fix_asset_tracks">
  <ol class="breadcrumb">
    <li><%= link_to '固定资产追踪列表', fix_asset_tracks_path %></li>
    <li>{{action}}</li>
  </ol>

  <hr class="hr-grey"/>

  <form>
    <fieldset>
      <legend style="width: 220px;">{{action}}</legend>
      <div>
        <b>选择数据文件：</b>
        <input id="item-data-uploader" type="file" name="files[]" data-url="/fix_asset_tracks/import"/>

        <hr class="hr-grey"/>

        <div id="item-data-uploader-preview"></div>

        <div>
          <b>下载模板：</b>
          <a href="/files/<%= ImportTemplate.fix_asset_track_excel_template %>">
            <%= ImportTemplate::FIX_ASSET_TRACK_EXCEL_TEMPLATE %></a>
        </div>
      </div>
    </fieldset>
  </form>

</div>

<script type="text/javascript">
  $(document).ready(function () {
    data_upload("#item-data-uploader");
  });
</script>