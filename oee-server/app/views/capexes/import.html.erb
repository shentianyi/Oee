<p id="notice"><%= notice %></p>

<div id="bu_mangers">
  <ol class="breadcrumb">
    <li>CAPEX</li>
    <li><%= link_to 'CAPEX', capexes_path %></li>
  </ol>

  <hr class="hr-grey"/>

  <form>
    <fieldset>

      <div>
        <b>选择数据文件：</b>
        <input id="item-data-uploader" type="file" name="files[]" data-url="/capexes/import"/>

        <hr class="hr-grey"/>

        <div id="item-data-uploader-preview">
          处理信息
        </div>
      </div>

      <div>
        <b>下载模板：</b>
        <a href="/files/<%= ImportTemplate.capex_budget_excel_template %>">
          <%= ImportTemplate::CAPEX_BUDGET_EXCEL_TEMPLATE %></a>
      </div>
    </fieldset>
  </form>
</div>

<script type="text/javascript">
  $(document).ready(function () {
    data_upload("#item-data-uploader");
  });
</script>