<p id="notice"><%= notice %></p>

<div>
  <ol class="breadcrumb">
    <li><%= link_to '月平衡报表', asset_balance_lists_path %></li>
  </ol>

  <hr class="hr-grey"/>

  <form>
    <fieldset>
      <div>
        <b>选择数据文件：</b>
        <input id="item-data-uploader" type="file" name="files[]" data-url="/asset_balance_lists/import"/>

        <hr class="hr-grey"/>

        <div id="item-data-uploader-preview">
          处理信息
        </div>
      </div>

      <div>
        <b>下载模板：</b>
        <a href="/files/<%= ImportTemplate.asset_balance_item_excel_template %>">
          <%= ImportTemplate::ASSET_BALANCE_ITEM_EXCEL_TEMPLATE %></a>
      </div>
    </fieldset>
  </form>
</div>

<script type="text/javascript">
  $(document).ready(function () {
    data_upload("#item-data-uploader");
  });
</script>