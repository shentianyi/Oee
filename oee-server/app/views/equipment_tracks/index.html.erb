<p id="notice"><%= notice %></p>

<h1>Equipment Tracks</h1>

<div class="marco-nheading">
  设备追踪表
  <div class="pull-right">
    <%= link_to content_tag(:i, content_tag(:span, "导入数据 "), :class => "fa fa-cloud-upload"), import_equipment_tracks_path %>

    <%= link_to content_tag(:i, content_tag(:span, "新建"), :class => "fa fa-plus-square"), new_equipment_track_path %>
  </div>
</div>

<table>
  <thead>
    <tr>
      <th>Level</th>
      <th>Name</th>
      <th>Nr</th>
      <th>Type</th>
      <th>Asset nr</th>
      <th>Description</th>
      <th>Product</th>
      <th>Equipment serial</th>
      <th>Supplier</th>
      <th>Status</th>
      <th>Profit center</th>
      <th>Department</th>
      <th>Project</th>
      <th>Location</th>
      <th>Area</th>
      <th>Position</th>
      <th>Procurment date</th>
      <th>Release cycle</th>
      <th>Next release</th>
      <th>Release notice</th>
      <th>Responsibilityer</th>
      <th>Remark</th>
      <th>Is top</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equipment_tracks.each do |equipment_track| %>
      <tr>
        <td><%= equipment_track.level %></td>
        <td><%= equipment_track.name %></td>
        <td><%= equipment_track.nr %></td>
        <td><%= equipment_track.type %></td>
        <td><%= equipment_track.asset_nr %></td>
        <td><%= equipment_track.description %></td>
        <td><%= equipment_track.product_id %></td>
        <td><%= equipment_track.equipment_serial_id %></td>
        <td><%= equipment_track.supplier %></td>
        <td><%= equipment_track.status %></td>
        <td><%= equipment_track.profit_center %></td>
        <td><%= equipment_track.department %></td>
        <td><%= equipment_track.project %></td>
        <td><%= equipment_track.location %></td>
        <td><%= equipment_track.area %></td>
        <td><%= equipment_track.position %></td>
        <td><%= equipment_track.procurment_date %></td>
        <td><%= equipment_track.release_cycle %></td>
        <td><%= equipment_track.next_release %></td>
        <td><%= equipment_track.release_notice %></td>
        <td><%= equipment_track.responsibilityer %></td>
        <td><%= equipment_track.remark %></td>
        <td><%= equipment_track.is_top %></td>
        <td><%= link_to 'Show', equipment_track %></td>
        <td><%= link_to 'Edit', edit_equipment_track_path(equipment_track) %></td>
        <td><%= link_to 'Destroy', equipment_track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


